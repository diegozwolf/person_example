<h1>People</h1>

<table class="table">
  <thead class="thead-dark">
    <tr>
      <th>id</th>
      <th>name</th>
      <th>last_name</th>
      <th>email</th>
      <th>Age</th>
      <th>gender</th>
    </tr>
  </thead>
  <tbody>
    <% @people.each do |person| %>
    <tr>
      <td><%= person.id %></td>
      <td><%= person.name%></td>
      <td><%= person.last_name %></td>
      <td><%= person.email %></td>
      <td><%= person.age %></td>
      <td><%= person.gender == true ? 'male' : 'female' %></td>
    </tr>
    <% end %>
  </tbody>
</table>
